{
    "$schema": "https://mintlify.com/schema.json",
    "name": "Interactly API",
    "logo": {
        "dark": "/logo/logo_dark.svg",
        "light": "/logo/logo_light.svg"
    },
    "favicon": "/logo/favicon.png",
    "modeToggle": {
        "default": "light"
    },
    "colors": {
        "primary": "#7e57c2",
        "light": "#7e57c2",
        "dark": "#7e57c2",
        "anchors": {
            "from": "#7e57c2",
            "to": "#7e57c2"
        }
    },
    "topbarLinks": [
        {
            "name": "Support",
            "url": "documentation/support"
        }
    ],
    "topbarCtaButton": {
        "name": "Dashboard",
        "url": "https://dashboard.interactly.ai"
    },
    "tabs": [
        {
            "name": "API Reference",
            "url": "api-reference",
            "openapi": "https://api-prod.interactly.ai/api-docs/api.json"
        }
    ],
    "anchors": [
        {
            "name": "Swagger",
            "icon": "code",
            "url": "https://api-prod.interactly.ai/api-docs"
        }
    ],
    "navigation": [
        {
            "group": "General",
            "pages": [
                "documentation/introduction",
                "documentation/support"
            ]
        },
        {
            "group": "Quickstart",
            "pages": [
                "documentation/dashboard",
                "documentation/client-SDK"
            ]
        },
        {
            "group": "Getting Started",
            "pages": [
                "api-reference/api-keys",
                "api-reference/verification-calls"
            ]
        }
    ],
    "api": {
        "playground": {
            "disableProxy": true
        }
    },
    "footerSocials": {}
}