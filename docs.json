{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "mint",
    "name": "Interactly API",
    "colors": {
        "primary": "#1E6EFF",
        "light": "#1E6EFF",
        "dark": "#1E6EFF"
    },
    "favicon": "/logo/favicon.png",
    "navigation": {
        "tabs": [
            {
                "tab": "Overview",
                "pages": [
                    "welcome"
                ]
            },
            {
                "tab": "Documentation",
                "groups": [
                    {
                        "group": "General",
                        "pages": [
                            "general/introduction",
                            "general/support"
                        ]
                    },
                    {
                        "group": "Quickstart",
                        "pages": [
                            "quickstart/dashboard",
                            "quickstart/client-SDK"
                        ]
                    },
                    {
                        "group": "Calls",
                        "pages": [
                            "calls/call-features"
                        ]
                    },
                    {
                        "group": "Webhooks",
                        "pages": [
                            "webhooks/server-url",
                            "webhooks/server-url/setting-server-urls",
                            "webhooks/server-url/events",
                            "webhooks/server-url/developing-locally"
                        ]
                    }
                ]
            },
            {
                "tab": "API Reference",
                "groups": [
                    {
                        "group": "Getting Started",
                        "pages": [
                            "api-reference/api-keys"
                        ]
                    },
                    {
                        "group": "APIs",
                        "openapi": {
                            "source": "https://api-dev.interactly.ai/api-docs/common/api.json",
                            "directory": "api-reference"
                        }
                    },
                    {
                        "group": "Campaigns",
                        "openapi": {
                            "source": "https://api-dev.interactly.ai/api-docs/campaigns/api.json",
                            "directory": "api-reference/campaigns"
                        }
                    },
                    {
                        "group": "Workflows",
                        "openapi": {
                            "source": "https://api-dev.interactly.ai/api-docs/workflows/api.json",
                            "directory": "api-reference/workflows"
                        }
                    },
                    {
                        "group": "Integrations",
                        "openapi": {
                            "source": "https://api-dev.interactly.ai/api-docs/integrations/api.json",
                            "directory": "api-reference/integrations"
                        }
                    },
                    {
                        "group": "Prompts",
                        "openapi": {
                            "source": "https://api-dev.interactly.ai/api-docs/prompts/api.json",
                            "directory": "api-reference/prompts"
                        }
                    }
                ],
                "global": {
                    "anchors": [
                        {
                            "anchor": "Swagger",
                            "href": "https://api-dev.interactly.ai/api-docs",
                            "icon": "code"
                        },
                        {
                            "anchor": "OpenAPI",
                            "href": "https://api-dev.interactly.ai/api-docs/api.json",
                            "icon": "code"
                        }
                    ]
                }
            },
            {
                "tab": "Changelog",
                "pages": [
                    "changelog/release-notes"
                ]
            }
        ]
    },
    "logo": {
        "light": "/logo/logo_light.svg",
        "dark": "/logo/logo_dark.svg"
    },
    "api": {
        "playground": {
            "display": "interactive",
            "proxy": false
        }
    },
    "appearance": {
        "default": "light"
    },
    "navbar": {
        "links": [
            {
                "label": "Website",
                "href": "https://interactly.ai"
            }
        ],
        "primary": {
            "type": "button",
            "label": "Dashboard",
            "href": "https://dashboard-dev.interactly.ai"
        }
    },
    "footer": {
        "socials": {}
    },
    "seo": {
        "indexing": "all"
    }
}